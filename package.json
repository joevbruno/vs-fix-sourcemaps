{
  "name": "vs-fix-sourcemaps",
  "version": "1.0.1",
  "description": "A Webpack plugin that fixes javascript sourcemaps in Visual Studio (2015), allowing for JSX and JS debugging directly in Visual Studio.",
  "main": "dist/index.js",
  "scripts": {
    "tests": "mocha ./__tests__/*.spec.js --compilers js:babel-core/register --timeout 15000:",
    "compile": "babel src/index.js -o dist/index.js",
    "lint": "eslint src/** || exit 0",
    "precommit": "npm run lint && npm run compile && npm run tests",
    "prepush": "npm run lint && npm run compile && npm run tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joevbruno/vs-fix-sourcemaps.git"
  },
  "keywords": [
    "Visual",
    "Studio",
    "JSX",
    "React",
    "JS",
    "Debugging",
    "Webpack"
  ],
  "author": "Joe Bruno <joevbruno@me.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joevbruno/vs-fix-sourcemaps/issues"
  },
  "homepage": "https://github.com/joevbruno/vs-fix-sourcemaps#readme",
  "dependencies": {
    "vlq": "^0.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "convert-source-map": "^1.2.0",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.3",
    "eslint-plugin-react": "^5.1.1",
    "expect": "^1.20.1",
    "husky": "^0.11.4",
    "mocha": "^2.5.3",
    "webpack": "^1.13.1"
  }
}
